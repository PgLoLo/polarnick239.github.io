---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>Notes about GPGPU development</title>
    <description>Notes about GPGPU development</description>
    <link>{{ site.url_en }}/blogs/gpgpu/index_en.html</link>
    <atom:link href="{{ "/blogs/gpgpu/feed_en.xml" | prepend: site.url_ }}" rel="self" type="application/rss+xml"/>
    <pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
    <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
    <generator>Jekyll v{{ jekyll.version }}</generator>
    {% for post in site.categories.gpgpu limit:10 %}
    {% if post.lang == 'en' %}
      <item>
        <title>{{ post.title | xml_escape }}</title>
        <description>{{ post.content | xml_escape }}</description>
        <pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
        <link>{{ post.url | prepend: site.url_en }}</link>
        <guid isPermaLink="true">{{ post.url | prepend: site.url_en }}</guid>
        {% for tag in post.tags %}
        <category>{{ tag | xml_escape }}</category>
        {% endfor %}
        {% for cat in post.categories %}
        <category>{{ cat | xml_escape }}</category>
        {% endfor %}
      </item>
    {% endif %}
    {% endfor %}
  </channel>
</rss>
